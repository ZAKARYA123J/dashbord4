{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/dashboard/updateor/[id]/page.jsx", "src/app/dashboard/show/Updatedetail.tsx", "src/components/dashboard/overview/latest-orders.jsx", "src/app/dashboard/orders/page.jsx", "src/app/dashboard/page.jsx", "src/components/core/theme-provider/theme-provider.jsx", "src/components/dashboard/integrations/integrations-filters.jsx", "src/components/dashboard/integrations/OrderDetails.jsx", "src/components/dashboard/layout/config.jsx"],
  "exclude": ["node_modules"]
}
